<Window x:Class="SkHttpClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkHttpClient"
        xmlns:view="clr-namespace:SkHttpClient.View"
        mc:Ignorable="d"
        Title="SKHTTP CLIENT BY MATEUSZ BEDNARSKI and ARTUR KONIECZNY" Height="350" Width="525">
    <Grid>
        <TabControl>
            <TabItem Header="GET">
                <view:GetView />
